<mvc:View controllerName="hr.teach.controller.Object" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:semantic="sap.m.semantic"
	xmlns:core="sap.ui.core">
	<semantic:FullscreenPage id="page" navButtonPress="onNavBack" showNavButton="true" title="{LessonDate}" busy="{objectView>/busy}"
		busyIndicatorDelay="{objectView>/delay}">
		<semantic:content>
			<ObjectHeader id="objectHeader" title="{LessonName}"></ObjectHeader>
			<IconTabBar class="sapUiResponsiveContentPadding">
				<items>
					<!-- Icon Tab Bary-->
				</items>
			</IconTabBar>
		</semantic:content>
	</semantic:FullscreenPage>
</mvc:View>